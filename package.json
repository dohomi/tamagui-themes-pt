{
  "name": "tamagui-themes-pt",
  "version": "1.0.1",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "v2.js",
    "v2.d.ts",
    "v2-themes.js",
    "v2-themes.d.ts",
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "generate": "yarn generate:v1 && yarn generate:v2",
    "generate:v1": "tamagui generate-themes ./src/themes-new.ts ./src/generated-new.ts",
    "generate:v2": "tamagui generate-themes ./src/v2-themes.ts ./src/generated-v2.ts",
    "build": "tamagui-build",
    "watch": "yarn build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./src/themes-new.ts": "./src/themes-new.ts",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./v2": {
      "types": "./types/v2.d.ts",
      "import": "./dist/esm/v2.js",
      "require": "./dist/cjs/v2.js"
    },
    "./v2-themes": {
      "types": "./types/v2-themes.d.ts",
      "import": "./dist/esm/v2-themes.js",
      "require": "./dist/cjs/v2-themes.js"
    }
  },
  "dependencies": {
    "@radix-ui/colors": "3.0.0",
    "@tamagui/colors": "1.76.0",
    "@tamagui/create-theme": "1.76.0",
    "@tamagui/theme-builder": "1.76.0",
    "@tamagui/web": "1.76.0"
  },
  "devDependencies": {
    "@tamagui/build": "1.76.0",
    "@tamagui/cli": "1.76.0",
    "@types/react": "^18.2.37",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vitest": "^0.34.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
